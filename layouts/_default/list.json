{
  "section": {{ .Title | jsonify }},
  "items": [
    {{- range $index, $page := .Pages -}}
    {{- if $index }},{{ end }}
    {
      "title": {{ $page.Title | jsonify }},
      "url": {{ $page.Permalink | jsonify }},
      "date": {{ $page.Date.Format "2006-01-02" | jsonify }},
      "prepTime": {{ $page.Params.prepTime | default 0 | jsonify }},
      "cookTime": {{ $page.Params.cookTime | default 0 | jsonify }},
      "categories": {{ $page.Params.categories | default slice | jsonify }},
      "cuisines": {{ $page.Params.cuisines | default slice | jsonify }},
      "carbs": {{ $page.Params.carbs | default slice | jsonify }},
      "proteins": {{ $page.Params.proteins | default slice | jsonify }},
      "diets": {{ $page.Params.diets | default slice | jsonify }},
      "methods": {{ $page.Params.methods | default slice | jsonify }}
    }
    {{- end }}
  ]
}
